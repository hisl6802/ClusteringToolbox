import tkinter as tk
from tkinter import ttk

root = tk.Tk()
root.title("Clustering Notebook")

# Create the notebook widget
notebook = ttk.Notebook(root)
notebook.pack(expand=True, fill="both")  # Expand to fill any space not otherwise used

# Create two frames for two tabs
frame1 = ttk.Frame(notebook)
frame2 = ttk.Frame(notebook)

# Add frames as pages for the notebook
notebook.add(frame1, text='Tab 1')
notebook.add(frame2, text='Tab 2')

# Add widgets to the first tab
label1 = ttk.Label(frame1, text="This is Tab 1")
label1.pack()

# Add widgets to the second tab
label2 = ttk.Label(frame2, text="This is Tab 2")
label2.pack()

root.mainloop()
